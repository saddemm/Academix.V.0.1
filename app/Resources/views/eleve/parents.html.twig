<div class="pars" data-prototype="{{ form_row(form.parents.vars.prototype)|e('html_attr') }}">
    {% for parent in form.parents %}
        <div class="par">

            <div class="col-lg-6">
                <div class="form-group">
                    <label for="l30">Nom</label>
                    {{ form_widget(parent.nom) }}
                </div>
            </div>
            <div class="col-lg-6">
                <div class="form-group">
                    <label for="l30">Prénom</label>
                    {{ form_widget(parent.prenom) }}
                </div>
            </div>
            <div class="col-lg-6">
                <div class="form-group">
                    <label for="l30">E-mail</label>
                    {{ form_widget(parent.email) }}
                </div>
            </div>
            <div class="col-lg-6">
                <div class="form-group">
                    <label for="l30">Téléphone</label>
                    {{ form_widget(parent.tel) }}
                </div>
            </div>
            <div class="col-lg-6">
                <div class="form-group">
                    <label class="form-control-label" for="l15">Adresse</label>
                    {{ form_widget(parent.adr) }}
                </div>
            </div>

            <div class="col-lg-6">
                <div class="form-group">
                    <label class="form-control-label" for="l15">Methode de contact</label>
                    {{ form_widget(parent.methodeContact) }}
                </div>
            </div>
            <div class="col-lg-12">
                <div class="form-group">
                    <label class="form-control-label" for="l15">Type</label>
                    {{ form_widget(parent.type) }}
                </div>
            </div>
            <div class="clearfix"></div>
            <div class="col-lg-12">
                <div class="form-group">
                    <label class="form-control-label" for="l15">Responsable</label>
                    {{ form_widget(parent.responsable) }}
                </div>
            </div>
            <div class="clearfix"></div>
        </div>


        <hr>
    {% endfor %}



</div>